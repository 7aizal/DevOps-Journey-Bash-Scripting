#!/bin/bash

read -p "Enter first number: " num1
read -p "Enter second number: " num2

echo "Addition: $((num1 + num2))"
echo "Subtraction: $((num1 - num2))"
echo "Multiplication: $((num1 * num2))"
if [ "$num2" -ne 0 ]; then
  echo "Division: $((num1 / num2))"
else
  echo "Division: Cannot divide by zero"
fi
